definition
	EVALUATION[at0000.1] matches {	-- Antecedentes Pessoais
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0.17] occurrences matches {0..*} matches {	-- Antecedentes Pessoais
						items cardinality matches {0..*; unordered} matches {
							ELEMENT[at0.2] occurrences matches {0..1} matches {	-- História Pregressa
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0.3, 	-- Diabetes Tipo 1
											at0.4, 	-- Diabetes Tipo 2
											at0.5, 	-- Hipertensão Arterial
											at0.6, 	-- Doença Cardíaca Isquêmica
											at0.7, 	-- Doença Trombo-Embólica
											at0.8, 	-- Epilepsia
											at0.9, 	-- Asma
											at0.10, 	-- DPOC
											at0.11]	-- Neoplasia Maligna
										}
									}
								}
							}
							ELEMENT[at0.16] occurrences matches {0..1} matches {	-- História Pregressa - CID
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[ac0.1]}		-- *CID 10
									}
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Observações
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Comentário sobre a resposta
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Sumário
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

