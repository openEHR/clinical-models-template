                                                    at0042;     -- Despido
                                                    at0031]    -- assumed value
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0039] occurrences matches {0..1} matches {    -- Temperatura Ambiente
                                        value matches {
                                            C_DV_QUANTITY <
                                                property = <[openehr::127]>
                                                list = <
                                                    ["1"] = <
                                                        units = <"°C">
                                                    >
                                                    ["2"] = <
                                                        units = <"°F">
                                                    >
                                                >
                                                assumed_value = <
                                                    precision = <-1>
                                                    magnitude = <0.0>
                                                    units = <"°C">
                                                >
