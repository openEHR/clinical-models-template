                                        value matches {
                                            C_DV_QUANTITY <
                                                property = <[openehr::127]>
                                                list = <
                                                    ["1"] = <
                                                        units = <"°C">
                                                    >
                                                    ["2"] = <
                                                        units = <"°F">
                                                    >
                                                >
                                            >
                                        }
                                    }
                                }
                            }
                        }
                        state matches {
                            ITEM_LIST[at0029] matches {    -- Condição
