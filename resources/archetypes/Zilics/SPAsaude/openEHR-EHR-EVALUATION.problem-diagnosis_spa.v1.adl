definition
    EVALUATION[at0000.1] matches {
        data matches {
            ITEM_TREE[at0001] matches {
                items existence matches {0..1} cardinality matches {1..*; ordered} matches {
                    CLUSTER[at0038.1] occurrences matches {0..*} matches {
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[at0039.1] occurrences matches {0..1} matches {
                                value matches {
                                    DV_CODED_TEXT matches {*}
                                }
                            }
                            ELEMENT[at0002.1] occurrences matches {1..1} matches {
                                value matches {
                                    DV_TEXT matches {*}
                                }
                            }
                            ELEMENT[at0042.1] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DATE_TIME matches {
                                        value matches {yyyy-??-??T??:??:??}
                                    }
                                }
                            }
                            ELEMENT[at0010.1] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DATE matches {
                                        value matches {yyyy-??-XX}
                                    }
                                }
                            }
                            ELEMENT[at0037.1] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DURATION matches {
                                        value matches {PT0S}
                                    }
                                }
                            }
                            ELEMENT[at0004.1] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DURATION matches {
                                        value matches {PT0S}
                                    }
                                }
                            }
                            ELEMENT[at0005.1] occurrences matches {0..1} matches {
                                value matches {
                                    1|[local::at0006],
                                    4|[local::at0007],
                                    7|[local::at0008]
                                }
                            }
                            CLUSTER[at0.35] occurrences matches {0..1} matches {
                                items cardinality matches {1..*; unordered} matches {
                                    ELEMENT[at0.36] occurrences matches {0..*} matches {
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                }
                            }
                            ELEMENT[at0.32] occurrences matches {0..1} matches {
                                value matches {
                                    DV_CODED_TEXT matches {
                                        defining_code matches {
                                            [local::
                                            at0.33,
                                            at0040.1]
                                        }
                                    }
                                }
                            }
                        }
                    }
                    CLUSTER[at0.37] occurrences matches {0..1} matches {
                        items cardinality matches {1..4; ordered} matches {
                            ELEMENT[at0.38] occurrences matches {0..1} matches {
                                value matches {
                                    DV_TEXT matches {*}
                                }
                            }
                            ELEMENT[at0.39] occurrences matches {0..1} matches {
                                value matches {
                                    -1|[local::at0.40],
                                    0|[local::at0.41],
                                    5|[local::at0.42],
                                    10|[local::at0.43],
                                    20|[local::at0.44],
                                    30|[local::at0.45],
                                    40|[local::at0.46]
                                }
                            }
                            ELEMENT[at0.47] occurrences matches {0..1} matches {
                                value matches {
                                    -1|[local::at0.48],
                                    0|[local::at0.49],
                                    10|[local::at0.50]
                                }
                            }
                            ELEMENT[at0.51] occurrences matches {0..1} matches {
                                value matches {
                                    -1|[local::at0.52],
                                    0|[local::at0.53],
                                    10|[local::at0.54]
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[at0032] matches {
                items existence matches {0..1} cardinality matches {0..*; unordered} matches {
                    CLUSTER[at0033] occurrences matches {0..1} matches {
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[at0034] occurrences matches {0..*} matches {
                                value matches {
                                    DV_TEXT matches {*}
                                }
                            }
                            ELEMENT[at0035] occurrences matches {0..*} matches {
                                value matches {
                                    DV_URI matches {*}
                                }
                            }
                        }
                    }
                }
            }
        }
    }

