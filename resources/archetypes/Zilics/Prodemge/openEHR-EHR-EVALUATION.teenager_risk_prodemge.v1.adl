ontology
    term_definitions = <
        ["en"] = <
            items = <
                ["at0000"] = <
                    text = <"Avaliação de Risco do Adolescente">
                    description = <"Usado para definir a avaliação de risco do adolescente">
                >
                ["at0001"] = <
                    text = <"ITEM_TREE">
                    description = <"@ internal @">
                >
                ["at0002"] = <
                    text = <"Estratificação de Risco do Aldolescente">
                    description = <"*">
                >
                ["at0003"] = <
                    text = <"Fatores de Risco Presentes">
                    description = <"*">
                >
                ["at0004"] = <
                    text = <"Adolescentes na faixa etária dos 10 aos 14 anos de idade">
                    description = <"*">
                >
                ["at0005"] = <
                    text = <"Tenham iniciado a atividade sexual precocemente, sem proteção para DST/Aids e gravidez">
                    description = <"*">
                >
                ["at0006"] = <
                    text = <"Tenham irmãs grávidas adolescentes ou que foram mães adolescentes">
                    description = <"*">
                >
                ["at0007"] = <
                    text = <"Estejam faltando com freqüência à escola, com evasão escolar e com problemas escolares">
                    description = <"*">
                >
                ["at0008"] = <
                    text = <"Residam em áreas de riscos à saúde e onde há aumento de violência">
                    description = <"*">
                >
                ["at0009"] = <
                    text = <"Inseridos em famílias desestruturadas">
                    description = <"*">
                >
                ["at0010"] = <
                    text = <"Estejam sofrendo ou em risco de sofrer violência doméstica">
                    description = <"*">
                >
                ["at0011"] = <
                    text = <"Tenham riscos nutricionais: anemia ferro-priva, hipovitaminoses, obesidade e desnutrição">
                    description = <"*">
                >
                ["at0012"] = <
                    text = <"Adolescentes com doenças crônicas e necessidades especiais">
                    description = <"*">
                >
                ["at0013"] = <
                    text = <"Adolescentes com doenças sexualmente transmissíveis ou Aids">
                    description = <"*">
                >
                ["at0014"] = <
                    text = <"Adolescentes com gravidez precoce não planejada">
                    description = <"*">
                >
                ["at0015"] = <
                    text = <"Adolescentes com transtornos alimentares: bulimia e anorexia">
                    description = <"*">
                >
                ["at0016"] = <
                    text = <"Estejam fazendo uso/abuso de substâncias lícitas ou ilícitas (com destaque ao uso do tabaco e do álcool)">
                    description = <"*">
                >
                ["at0017"] = <
                    text = <"Sejam vítimas de exploração sexual ou que tenham sofrido abuso sexual">
                    description = <"*">
                >
                ["at0018"] = <
                    text = <"Com quadros de depressão">
                    description = <"*">
                >
                ["at0019"] = <
                    text = <"Tenham transtornos mentais e/ou risco de suicídio">
                    description = <"*">
                >
                ["at0020"] = <
                    text = <"Fogem com freqüência de casa ou se encontrem morando nas ruas">
                    description = <"*">
                >
                ["at0021"] = <
                    text = <"Estratificação de Risco">
                    description = <"*">
                >
                ["at0022"] = <
                    text = <"Sem Risco">
                    description = <"*">
                >
                ["at0023"] = <
                    text = <"Risco">
                    description = <"*">
                >
                ["at0024"] = <
                    text = <"Alto Risco">
                    description = <"Adolescente de alto risco">
                >
            >
        >
