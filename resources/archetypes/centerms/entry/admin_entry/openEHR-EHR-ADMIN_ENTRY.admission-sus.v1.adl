											[local::
											at0141, 	-- *Unidade Básica de Saúde(en)
											at0142, 	-- *Creche/Escola(en)
											at0143, 	-- *Na Rua(en)
											at0144, 	-- *Unidade Móvel(en)
											at0145, 	-- *Domicílio(en)
											at0146]	-- *Outros(en)
										}
									}
								}
							}
							ELEMENT[at0077] occurrences matches {0..1} matches {	-- Ward
