archetype (adl_version=1.4)
	openEHR-EHR-EVALUATION.obstetric_summary.v1

concept
	[at0000]	-- Obstetric summary
language
	original_language = <[ISO_639-1::en]>
	translations = <
		["pt-br"] = <
			language = <[ISO_639-1::pt-br]>
			author = <
				["name"] = <"Fátima Almeida">
				["organisation"] = <"Critical Software">
